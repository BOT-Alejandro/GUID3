<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GUID3" parent="Theme.Material3.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/AzulPrincipal</item>
        <item name="colorPrimaryVariant">@color/AzulSecundario</item>
        <item name="colorOnPrimary">@color/blanco</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/AzulPrincipal</item>
        <item name="colorSecondaryVariant">@color/AzulSecundario</item>
        <item name="colorOnSecondary">@color/negro</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="FormaCuadradaFAB" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerSize">120%</item>
    </style>
</resources>